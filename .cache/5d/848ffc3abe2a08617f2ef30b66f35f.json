: 700;\n      font-size: 27px;\n    }\n  }\n}\n","assets/css/loader.scss":"@import \"./base.scss\";\n.loading {\n  position: fixed;\n  top: 0;\n  left: 0;\n  bottom: 0;\n  right: 0;\n  height: 100vh;\n  width: 100%;\n  display: grid;\n  place-items: center;\n  z-index: 99;\n  background-color: #0b134f;\n  font-family: \"Orbitron\", sans-serif;\n  display: none;\n  &__box {\n    position: relative;\n    width: 500px;\n    height: 200px;\n    border: 3px solid $green-color;\n    border-top: 3px solid $dark-green-color;\n    border-bottom: 3px solid $dark-green-color;\n  }\n  &__bar {\n    width: 90%;\n    height: 10px;\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translateX(-50%);\n    background-color: #ccc;\n    border-radius: 2px;\n    &--inner {\n      height: 100%;\n      width: 50%;\n      background-color: $green-color;\n      border-radius: 2px;\n    }\n  }\n  &__text {\n    position: relative;\n    color: #fff;\n    padding: 1rem;\n    font-size: 20px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-weight: 700;\n    &--dot {\n      width: 15px;\n      height: 15px;\n      border: 0 3px;\n      border-radius: 50%;\n      background-color: #fff;\n      animation: pulse 0.8s infinite;\n      @keyframes pulse {\n        from {\n          opacity: 0;\n          background-color: $green-color;\n        }\n        to {\n          opacity: 1;\n        }\n      }\n    }\n    &--border {\n      width: 85%;\n      height: 1px;\n      background: $dark-green-color;\n      position: absolute;\n      bottom: 0;\n      left: 50%;\n      transform: translateX(-50%);\n    }\n  }\n  &__counter {\n    position: absolute;\n    top: 70%;\n    left: 0;\n    color: #fff;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    width: 100%;\n    font-size: 20px;\n    font-weight: 700;\n    padding: 1rem;\n    &--number {\n      color: $green-color;\n    }\n  }\n  &__width_animation {\n    animation: width_animation 1s forwards;\n    @keyframes width_animation {\n      0% {\n        width: 85%;\n      }\n      100% {\n        width: 280px;\n      }\n    }\n  }\n  &__svg {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    width: 85%;\n    opacity: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c89c1105b42067bbb27d38a2eac34921","cacheData":{"env":{}}}